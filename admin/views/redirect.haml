%head
  %title Redirection
%body
  -if @verbose
    %h2 Response:
    =@verbose
  %form{:action=>@url,:method=>'post',:name=>'primum',:enctype=>'text/plain'}
    -@post.each do |k,v|
      -if @verbose
        %p
          %strong= k
          %span= v
      %input{:type=>'hidden',:name=>k,:value=>v}
    %input{:type=>"submit",:value=>"Please wait while redirecting to G.B Premium"}
